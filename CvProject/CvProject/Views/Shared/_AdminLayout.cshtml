
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Admin Paneli</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Font Awesome -->
    <link rel="stylesheet" href="/admin-template/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="/admin-template/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="/Default/Index/" class="brand-link" target="_blank">
                <img src="/admin-template/dist/img/AdminLTELogo.png"
                     alt="AdminLTE Logo"
                     class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">Siteye Git</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="~/resume-template/assets/img/image.jpeg" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="/About/Index/" class="d-block">Admin Paneli</a>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item has-treeview">
                            <a href="/About/Index/" class="nav-link">
                                <i class="nav-icon fas fa-comment-alt"></i>
                                <p>
                                    Hakkımda
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Experience/Index/" class="nav-link">
                                <i class="nav-icon fas fa-passport"></i>
                                <p>
                                    İş Deneyimleri
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Education/Index/" class="nav-link">
                                <i class="nav-icon fas fa-school"></i>
                                <p>
                                    Eğitim Bilgileri
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Skill/Index/" class="nav-link">
                                <i class="nav-icon fas fa-expand"></i>
                                <p>
                                    Yetkinlikler
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Interest/Index/" class="nav-link">
                                <i class="nav-icon fas fa-bars"></i>
                                <p>
                                    İlgi Alanları
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Certification/Index/" class="nav-link">
                                <i class="nav-icon fas fa-award"></i>
                                <p>
                                    Sertifikalar Ve Kursları
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Contact/Index/" class="nav-link">
                                <i class="nav-icon far fa-address-book"></i>
                                <p>
                                    İletişim
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Admin/Index/" class="nav-link">
                                <i class="nav-icon fas fa-pen"></i>
                                <p>
                                    Ayarlar
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <br />
                        </li>
                        <li class="nav-item">
                            <a href="/Account/LogOut/" class="nav-link">
                                <i class="nav-icon fas fa-lock"></i>
                                <p>
                                    Çıkış Yap
                                </p>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Main content -->
            <section class="content">
                <div class="row">
                    <div class="col-md-12">
                        @RenderBody()
                    </div>
                </div>
            </section>
        </div>
    </div>
    <!-- jQuery -->
    <script src="/admin-template/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="/admin-template/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/admin-template/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="/admin-template/dist/js/demo.js"></script>
    <script src="/admin-template/dist/js/demo.js"></script>
</body>
</html>

